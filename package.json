{
    "name": "RNDemo",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "tcp": "adb reverse tcp:8081 tcp:8081",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "export-huskyrc": "echo export PATH=\"/usr/local/bin:$PATH\" >> ~/.huskyrc",
        "prepare": "husky install",
        "detox-build-i": "detox build --configuration ios.sim.debug",
        "detox-build-a": "detox build --configuration android.emu.debug",
        "detox-test-i": "detox test --configuration ios.sim.debug",
        "detox-test-a": "detox test --configuration android.emu.debug"
    },
    "dependencies": {
        "@react-navigation/native": "^6.1.1",
        "@react-navigation/native-stack": "^6.9.6",
        "@reduxjs/toolkit": "^1.9.0",
        "react": "18.1.0",
        "react-native": "0.70.6",
        "react-native-gesture-handler": "^2.8.0",
        "react-native-responsive-linechart": "^5.7.1",
        "react-native-safe-area-context": "^4.4.1",
        "react-native-screens": "^3.18.2",
        "react-native-sensitive-info": "^6.0.0-alpha.9",
        "react-native-svg": "^13.6.0",
        "react-native-vector-icons": "^9.2.0",
        "react-redux": "^8.0.5",
        "react-test-renderer": "18.1.0",
        "redux": "^4.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.2",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@babel/runtime": "^7.12.5",
        "@react-native-community/eslint-config": "^2.0.0",
        "@testing-library/react-hooks": "^8.0.1",
        "@tsconfig/react-native": "^2.0.2",
        "@types/detox": "^18.1.0",
        "@types/jest": "^26.0.23",
        "@types/react": "^18.0.21",
        "@types/react-native": "^0.70.6",
        "@types/react-native-vector-icons": "^6.4.12",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.37.0",
        "@typescript-eslint/parser": "^5.37.0",
        "babel-jest": "^29.3.1",
        "babel-plugin-module-resolver": "^4.1.0",
        "detox": "^20.1.2",
        "eslint": "^7.32.0",
        "husky": "^8.0.2",
        "jest": "^29.3.1",
        "metro-react-native-babel-preset": "0.72.3",
        "ts-jest": "^29.0.3",
        "typescript": "^4.8.3"
    },
    "jest": {
        "setupFiles": [
            "./__mock__/mock.js"
        ],
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "testPathIgnorePatterns": [
            "./e2e"
        ],
        "allowJs": true,
        "moduleNameMapper": {
            "\\.(png|jpg|ico|jpeg|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mock/ImageMock.js"
        }
    }
}
